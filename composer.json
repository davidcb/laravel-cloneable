{
    "name": "davidcb/laravel-cloneable",
    "description": "Trait to enable cloneable behaviour for Laravel models",
    "homepage": "https://github.com/davidcb/laravel-cloneable",
    "type": "library",
    "require": {
        "illuminate/support": "^9.0|^10.0",
        "illuminate/database": "^9.0|^10.0",
        "spatie/laravel-medialibrary": "^10.0|^11.0",
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Davidcb\\LaravelCloneable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Davidcb\\LaravelCloneable\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "David Couto",
            "email": "davidcouto@gmail.com",
            "role": "Full Stack Developer"
        }
    ],
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Davidcb\\LaravelCloneable\\CloneableServiceProvider"
            ]
        }
    }
}
